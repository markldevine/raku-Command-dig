#!/usr/bin/env raku

use lib '/home/mdevine/github.com/raku-Command-dig/lib';
use Command::dig;

my Command::dig $digger    .= new(
                                     :dns-servers<170.121.18.30 170.121.218.30>,
                                     :dns-domains<wmata.com wmata.local ncs.wmata.com video.wmata.com>,
                                 );

$digger.dns-servers         = <10.10.43.40 10.10.43.40>;
$digger.dns-domains         = <wmata.com wmata.local>;

#dd $digger.resolve('10.11.101.230');
#dd $digger.resolve('10.10.137.41');
#dd $digger.resolve('10.10.137.41');
#dd $digger.resolve('10.11.105.96');
#dd $digger.resolve('170.121.1.194');

#dd $digger.resolve('isplc02');
#dd $digger.resolve('P520TSMJGB');
#dd $digger.resolve('JATSMPRD03');
#dd $digger.resolve('ctstmgtgate1lpv');
#dd $digger.resolve('nimjgb');
dd $digger.resolve('p650nimjgb');

=finish

new(ip-address => "10.11.101.230", alias-names => {}, canonical-name => "isplc02.wmata.com")

new(ip-address => "10.10.137.41", alias-names => {"p520tsmjgb.wmata.com" => ${:canonical-name("jatsmprd03.wmata.com"), :ip-address("10.10.137.41")}}, canonical-name => "jatsmprd03.wmata.com")

new(ip-address => "10.10.137.41", alias-names => {}, canonical-name => "jatsmprd03.wmata.com")

new(ip-address => "10.11.105.96", alias-names => {}, canonical-name => "ctstmgtgate1lpv.wmata.local")

new(ip-address => "170.121.1.194", alias-names => {"nimjgb.wmata.com" => ${:canonical-name("p650nimjgb.wmata.com"), :ip-address("170.121.1.194")}}, canonical-name => "p650nimjgb.wmata.com")

new(ip-address => "170.121.1.194", alias-names => {}, canonical-name => "p650nimjgb.wmata.com")
